import { ElementRef, AfterViewInit, EventEmitter, Renderer2 } from '@angular/core';
export declare class MdbColorSliderComponent implements AfterViewInit {
    private renderer;
    private el;
    private platformId;
    handle: ElementRef;
    slider: ElementRef;
    canvas: ElementRef<HTMLCanvasElement>;
    drag: MdbColorDraggableDirective;
    color: EventEmitter<string>;
    private mousedown;
    private ctx;
    private sliderParent;
    private previousImageData;
    isBrowser: boolean;
    constructor(renderer: Renderer2, el: ElementRef, platformId: any);
    handleMouseDown(): void;
    moveColorCircleToPosition(event: any): void;
    handleMouseUp(): void;
    handleMouseMove(event: any): void;
    private draw;
    ngAfterViewInit(): void;
    private setCanvasParentsWidth;
    private emitColor;
    private getColorAtPosition;
}
