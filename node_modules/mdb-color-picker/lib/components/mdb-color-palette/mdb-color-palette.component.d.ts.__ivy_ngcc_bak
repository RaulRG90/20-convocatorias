import { MdbColorPickerService } from './../../services/mdb-color-picker.service';
import { ElementRef, AfterViewInit, SimpleChanges, OnChanges, EventEmitter, Renderer2 } from '@angular/core';
export declare class MdbColorPaletteComponent implements AfterViewInit, OnChanges {
    private renderer;
    private colorService;
    private platformId;
    hue: string;
    color: EventEmitter<string>;
    canvas: ElementRef<HTMLCanvasElement> | any;
    private ctx;
    private mousedown;
    isBrowser: boolean;
    selectedPosition: {
        x: number;
        y: number;
    };
    onmouseup(): void;
    constructor(renderer: Renderer2, colorService: MdbColorPickerService, platformId: any);
    private draw;
    ngOnChanges(changes: SimpleChanges): void;
    onMouseDown(event: any): void;
    onMouseMove(event: any): void;
    protected getColorAtPosition(x: number, y: number): string;
    private pickColorAtStart;
    ngAfterViewInit(): void;
}
