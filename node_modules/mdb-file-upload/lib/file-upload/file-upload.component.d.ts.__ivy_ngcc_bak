import { ElementRef, EventEmitter, OnChanges, SimpleChanges } from '@angular/core';
import { FaIconLibrary } from '@fortawesome/angular-fontawesome';
export interface ITextTranslation {
    fileText?: string;
    removeText?: string;
    imageFileText?: string;
}
export declare class MdbFileUploadComponent implements OnChanges {
    fileInput: ElementRef;
    disabled: boolean;
    height: number;
    get defaultFile(): string;
    set defaultFile(value: string);
    private _defaultFile;
    customText: ITextTranslation;
    textTranslation: ITextTranslation;
    fileAdded: EventEmitter<File>;
    fileRemoved: EventEmitter<any>;
    imageFileExtensions: string[];
    url: string | ArrayBuffer;
    fileType: string;
    fileName: string;
    dragging: boolean;
    isFileAdded: boolean;
    isImageFile: boolean;
    defaultPreview: boolean;
    constructor(library: FaIconLibrary);
    private _setDefaultPreview;
    ngOnChanges(changes: SimpleChanges): void;
    onFilesAdded(): void;
    onFileDrop(event: any): void;
    onDragOver(event: any): void;
    onDragLeave(event: any): void;
    reset(): void;
    showPreview(file: any): void;
    removePreview(): void;
    getFileType(fileName: string): string;
    isImage(fileName: string): boolean;
    private _cleanFileName;
}
