import { OnInit, ElementRef, AfterContentInit, EventEmitter, OnDestroy } from '@angular/core';
import { MdbSortableService } from './mdb-sortable.service';
export declare class MdbSortableListDirective implements OnInit, AfterContentInit, OnDestroy {
    private el;
    private _document;
    private sortableService;
    private _sortables;
    autoScroll: boolean;
    connectedWith: MdbSortableListDirective[];
    data: any[];
    drop: EventEmitter<any>;
    private destroy$;
    private _previousList;
    private _currentList;
    private _currentIndex;
    private _newIndex;
    private _scrollSpeed;
    private _scrollSensitivity;
    currentIndex: number;
    newIndex: number;
    constructor(el: ElementRef, _document: any, sortableService: MdbSortableService);
    ngOnInit(): void;
    ngAfterContentInit(): void;
    ngOnDestroy(): void;
    private _sortItems;
    private _updateScrollPos;
    private _isPointerOverItem;
    private onSort;
    onDragEnd(): void;
}
